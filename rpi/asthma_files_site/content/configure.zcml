<configure
    xmlns:browser="http://namespaces.zope.org/browser">

    <browser:page
        for="*"
        name="image-artifact-view"
        permission="zope2.Public"
        class=".image_artifact.ImageArtifactView"
        />

    <browser:page
        for="*"
        name="annotation-view"
        permission="zope2.Public"
        class=".annotation_view.AnnotationView"
        />

</configure>
